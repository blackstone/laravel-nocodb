{
    "name": "blackstone/laravel-nocodb",
    "description": "NocoDB Eloquent Driver for Laravel",
    "license": "MIT",
    "authors": [
        {
            "name": "Developer",
            "email": "dev@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/support": "^11.0|^12.0",
        "illuminate/database": "^11.0|^12.0",
        "illuminate/http": "^11.0|^12.0",
        "illuminate/pagination": "^11.0|^12.0",
        "guzzlehttp/guzzle": "^7.8"
    },
    "require-dev": {
        "orchestra/testbench": "^9.0",
        "mockery/mockery": "^1.6",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "BlackstonePro\\NocoDB\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "BlackstonePro\\NocoDB\\Providers\\NocoDBServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}